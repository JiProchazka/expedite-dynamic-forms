{
  "title": "Expedite Dynamic Form",
  "button": "Send",
  "form": {
    "fields": [
      {
        "type": "text",
        "label": "Name",
        "name": "name",
        "validations": {
          "required": true,
          "min": 3
        },
        "defaultValue": "@"
      },
      {
        "type": "text",
        "label": "Address",
        "name": "address",
        "validations": {
          "required": true
        }
      },
      {
        "type": "textarea",
        "rows": 10,
        "label": "Message",
        "name": "message",
        "validations": {}
      },
      {
        "type": "switch",
        "label": "Agree with Terms",
        "name": "terms",
        "validations": {},
        "defaultValue": true
      },
      {
        "type": "select",
        "label": "City",
        "name": "city",
        "defaultValue": "praha",
        "options": [
          {
            "name": "Praha",
            "value": "praha"
          },
          {
            "name": "New York",
            "value": "new-work"
          },
          {
            "name": "London",
            "value": "london"
          }
        ]
      }
    ]
  }
}
